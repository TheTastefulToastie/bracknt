class Example
    static String fizzbuzz(int n)
        switch Math.min(1, n % 3) + Math.min(1, n % 5) * 2
            case 0: 
                return "FIZZBUZZ";
            case 1:
                return "FIZZ";
            case 2:
                return "BUZZ";
            default:
                return Integer.toString(n);

    public static void main(String[] args)
        for int i = 0; i < 20; ++i
            System.out.printf("[%d] %s\n", i, fizzbuzz(i));
